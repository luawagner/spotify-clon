---
interface Props {
    href?: string;
}
const { href } = Astro.props;
---

<li>
    <a
        class="flex gap-4 fill-zinc-400
     text-zinc-400 hover:fill-zinc-100 hover:text-zinc-100 items-center py-3 px-5 font-medium transition duration-300"
        href={href}
    >
        <slot />
    </a>
</li>
